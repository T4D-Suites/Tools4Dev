_t4dSrcSlackNotif(){
# Generated From $Tools4Dev_PATH/Templates/function.env
# v1.5.0
###### _t4dSrcSlackNotif
#   - Name:
#       _t4dSrcSlackNotif
###
#   - Synopsis:
#       Send Notif threw slack workspace
###
#   - Example:
#       _t4dSrcSlackNotif "$(_t4dSrcSlackDefaultMessage)" "$WS_SLACK_BASE_URL/{CUSTOM_CHANNEL_TOKEN}"
###
###### DOCUMENTATION BLOC #####
    local _Message="$1"
    local _ChannelUrl="$2"
    local _result="$(curl -X POST -H 'Content-type: application/json' --data "${_Message}" "${_ChannelUrl}" 2> /dev/null )"
    case $_result in 
        ok)
            _t4dDebugLog $psucceed "Notification Sent"
        ;;
        *)
            _t4dDebugLog $perror "$_result"
        ;;
    esac

###############################
}

